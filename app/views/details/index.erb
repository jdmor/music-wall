<% if current_user %>
  <div class='form-wrapper'>
    <form>
      <label>Review</label>
      <textarea class='form_control'></textarea>
    </form>
  </div>
<% else %>
  <p>Sign in to leave a review</p>
<% end %>

<h3>Reviews</h3>
<p><%= @song.title %> - <%= @song.author %></p>
<!-- loop through each review -->